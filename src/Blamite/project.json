{
	"version": "1.0.0",
	"title": "Blamite",
	"description": "DotNet library for interacting with Halo data.",
	"authors": [
		"Aaron Dierking",
		"TJ Tunnell",
		"Alex Forbes-Reed",
		"Lord Zedd"
	],
	
	"packOptions": {
		"summary": "DotNet library for interacting with Halo data.",
		"tags": [
			"halo",
			"blam",
			"modding",
			"xbox"
		],
		"repository": {
			"type": "git",
			"url": "https://github.com/XboxChaos/Assembly"
		}
	},

	"buildOptions": {
		"allowUnsafe": true,
		"debugType": "portable"
	},

	"dependencies": { },

	"frameworks": {
		"netstandard1.6": {
			"buildOptions": {
				"define": [ "NETSTANDARD1_5" ]
			},
			"dependencies": {
				"NETStandard.Library": "1.6.0",
				"System.Security.Cryptography.Algorithms": "4.2.0"
			},
			"imports": [
				"dnxcore50",
				"portable-net451+win8+win81+win10"
			]
		},
		"net45": {
			"buildOptions": {
				"define": [ "NET45" ]
			},
			"frameworkAssemblies": {
				"System.Data": "",
				"System.Numerics": "",
				"System.Runtime.Serialization": "",
				"System.Xml": "",
				"System.Xml.Linq": ""
			}
		}
	},
  "runtimes": {
    "win-x86": { },
	"win-x64": { } 
  }
}
